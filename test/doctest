js> SQLPoet.create().SELECT.sysdate.FROM.dual.toSql();
SELECT sysdate FROM dual

js> var tableName = 'employ';
js> SQLPoet.create()
  >   .SELECT['COUNT(*)'].AS('cnt')
  >   .FROM.user_tables
  >   .WHERE.table_name['='].$(tableName)
  > .toSql();
SELECT COUNT(*) AS cnt FROM user_tables WHERE table_name = 'employ'
